	<form method="get" action="/quizes">
<input type="text" id="buscador" name="search" placeholder="buscar"/>
<input type="submit" value="Buscar"/>
</form>
<p>
<%= search %>
</p>
    <table>
   <% var i; for (i=0; i < quizes.length; i++) { %>
<tr>
<td>
   <a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a>
</td>
<td><%= quizes[i].categorias %></td>
<td><a href="quizes/<%= quizes[i].id %>/edit"><button class="ocultars">editar</button></a></td>
<td>
<form method="post" action="quizes/<%= quizes[i].id %>?_method=delete">
<button type="submit" class="ocultar" onClick="return confirm('Borrar: <%= quizes[i].pregunta %>');">borrar</button>
</form>
</td>
</tr>
<% } %>
</table>
<p/>
<a href="/quizes/new"><button class="ocultar">Crear pregunta</button></a>
  
